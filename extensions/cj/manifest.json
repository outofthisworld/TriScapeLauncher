{
   "background": {
      "persistent": false,
      "scripts": [ "bg.js" ]
   },
   "browser_action": {
      "default_icon": "cheerpj-gray.png",
      "default_popup": "popup.html",
      "default_title": "Click to run applets"
   },
   "content_security_policy": "script-src 'self' http://cheerpjdeploy.leaningtech.com/ 'unsafe-eval' https://ssl.google-analytics.com; object-src 'self'",
   "description": "Runs Java applets without a JVM on any platform",
   "icons": {
      "128": "cheerpj.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "CheerpJ Applet Runner Beta",
   "optional_permissions": [ "http://*/", "https://*/" ],
   "permissions": [ "declarativeContent", "activeTab" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.18"
}
